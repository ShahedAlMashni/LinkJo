    <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav mr-auto">
            <li class="nav-item active">
                <a class="nav-link" href="/events">
                    Events
                </a>
            </li>
            <li class="nav-item active">
                <a class="nav-link" href="/volunteer">
                    Call For Volunteers
                </a>
            </li>
            <li class="nav-itme active">
                <a class="nav-link" href="/forum">
                    Forums
                </a>
            </li>
            <% if(locals.user){%>
            <li class="nav-itme active">
                <a class="nav-link" href="/bitlink">
                    Bitlinks
                </a>
            </li>

           <%}%>

        </ul>
        <ul class="navbar-nav ml-auto">
            <% if(locals.user){%>
                <li class="nav-itme active" style="float: right">
                    <a class="nav-link" href="/profile/<%= locals.user%>">
                        <%= locals.user%>
                    </a>
                </li>
                <%} else {%>
            <li class="nav-itme active" style="float: right">
                <a class="nav-link" href="/login">
                    Login
                </a>
            </li>
                <%}%>
